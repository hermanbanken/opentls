  "devDependencies": {
    "browserify": "^13.1.0",
    "tsify": "^1.0.7",
    "typescript": "^2.0.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "@types/es6-promise": "0.0.32",
    "pinkie-promise": "^2.0.1"
